<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bookstore.bookstore.dao.CategoriesMapper">

    <select id="findByBookId" resultType="com.bookstore.bookstore.dao.model.Categories">
        select * from categorie where book_id=#{bookId}
